{
  "name": "loteria",
  "version": "1.0.0",
  "repository": "https://github.com/stephanrossi/loteria",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "axios": "^0.26.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "mustache-express": "^1.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mustache-express": "^1.2.1",
    "@types/node": "^17.0.21",
    "nodemon": "^2.0.15",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "dev2": "ts-node-dev --transpile-only --cls --respawn --watch 'json,mst' src/server.ts",
    "dev": "nodemon --watch src/** --ext js,ts,mst,json,css --ignore src/**/*.spec.ts --exec ts-node-dev --transpile-only --cls --respawn src/server.ts",
    "build": "tsc --build",
    "clean": "tsc --build --clean"
  }
}
