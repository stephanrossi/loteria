generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mega {
  id                    Int      @id @default(autoincrement())
  concurso              Int      @unique
  date                  String
  dezenas               String[]
  acumulou              Boolean
  acumuladaProxConcurso String
  created_at            DateTime @default(now())

  @@map("mega_sena")
}

model Lotomania {
  id                      Int      @id @default(autoincrement())
  concurso                Int      @unique
  date                    String
  dezenas                 String[]
  acumulou                Boolean
  acumulado_prox_concurso String
  created_at              DateTime @default(now())

  @@map("lotomania")
}

model Quina {
  id                      Int      @id @default(autoincrement())
  concurso                Int      @unique
  date                    String
  dezenas                 String[]
  acumulou                Boolean
  acumulado_prox_concurso String
  created_at              DateTime @default(now())

  @@map("quina")
}

model Lotofacil {
  id                      Int      @id @default(autoincrement())
  concurso                Int      @unique
  date                    String
  dezenas                 String[]
  acumulou                Boolean
  acumulado_prox_concurso String
  created_at              DateTime @default(now())

  @@map("lotofacil")
}
